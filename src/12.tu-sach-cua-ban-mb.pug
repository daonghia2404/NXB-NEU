extends includes/layouts/Layout.pug

block pages
  .ProfilePage.layout
    .container  
      +Breadcrumb({ data: [
        { link: '#', title: 'Trang chủ' },
        { link: '#', title: 'Tủ sách của bạn' },
      ] })
      .layout-wrapper.flex 
        .layout-col.main
          .ProfileInfomationPage-wrapper 
            .CardWithTitle.tab-wrapper
              .CardWithTitle-header.flex.items-center.justify-between
                .CardWithTitle-header-title TỦ SÁCH CỦA BẠN
                .CardWithTitle-header-layout.flex.items-center 
